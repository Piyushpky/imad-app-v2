IP="`wget -qO- http://169.254.169.254/latest/meta-data/local-ipv4`"
JAVA_OPTS="<%="#{node['avreg_service']['java']['opts']}"%> -Djavax.net.ssl.trustStore=<%="#{node['avreg_service']['keystore']['location']}"%> -Dhealthcheck.config.json.path=<%="#{node['tomcat']['catalina_base']}/healthcheckconfig.json"%>"
CATALINA_OPTS="<%="#{node['avreg_service']['catalina']['opts']}"%> -Djava.rmi.server.hostname=$IP"
export ENVIRONMENT_NAME=<%="#{node['avreg_service']['environment']}"%>